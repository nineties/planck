/// Test of phi instruction.
/// Since the previous block of `exit` is `iftrue` in this program,
/// the right hand side of
///
/// %2 = phi(iftrue: %0, iffalse: %1)
///
/// is %0
export fun main(): i32 {
root:
    if true iftrue iffalse
iftrue:
    %0 = 123:i32
    goto exit
iffalse:
    %1 = 456:i32
    goto exit
exit:
    %2 = phi(iftrue: %0, iffalse: %1)
    return %2
}
